<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/logintest"
        tools:context=".MainActivity">

    <Button
            android:text="Log In"
            android:layout_width="132dp"
            android:layout_height="wrap_content"
            tools:layout_editor_absoluteX="167dp" android:id="@+id/loginSubmit"
            app:layout_constraintTop_toBottomOf="@+id/button2"
            android:layout_alignParentStart="true" android:layout_marginLeft="142dp" android:layout_marginStart="142dp"
            android:layout_alignParentLeft="true" android:layout_marginTop="469dp"
            android:layout_alignParentTop="true" android:textColor="#FFFFFF"
            android:background="@color/design_default_color_primary_dark"/>
    <ImageView
            android:layout_width="118dp"
            android:layout_height="85dp" app:srcCompat="@drawable/wiselogo" android:id="@+id/imageView"
            android:layout_marginTop="5dp" android:layout_alignParentTop="true" android:layout_alignParentStart="true"
            android:layout_marginLeft="133dp" android:layout_marginStart="133dp" android:layout_alignParentLeft="true"/>
    <ImageView
            android:layout_width="163dp"
            android:layout_height="141dp" app:srcCompat="@drawable/cfl" android:id="@+id/imageView2"
            android:layout_alignParentStart="true" android:layout_marginLeft="109dp" android:layout_marginStart="109dp"
            android:layout_alignParentLeft="true" android:layout_marginTop="100dp"
            android:layout_alignParentTop="true"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Name"
            android:ems="10"
            android:id="@+id/editText4" android:layout_alignParentStart="true" android:layout_marginLeft="89dp"
            android:layout_marginStart="89dp" android:layout_alignParentLeft="true" android:layout_marginTop="286dp"
            android:layout_alignParentTop="true"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPassword"
            android:ems="10"
            android:id="@+id/editText5" android:layout_alignParentStart="true" android:layout_marginLeft="84dp"
            android:layout_marginStart="84dp" android:layout_alignParentLeft="true" android:layout_marginTop="363dp"
            android:layout_alignParentTop="true"/>

</RelativeLayout>